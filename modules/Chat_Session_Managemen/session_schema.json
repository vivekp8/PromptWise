{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["session_id", "user_id", "start_time", "status"],
  "properties": {
    "session_id": { "type": "string" },
    "user_id": { "type": "string" },
    "start_time": { "type": "string", "format": "date-time" },
    "end_time": { "type": "string", "format": "date-time" },
    "status": {
      "type": "string",
      "enum": ["active", "ended"]
    },
    "history": {
      "type": "array",
      "items": { "type": "string" }
    }
  }
}
